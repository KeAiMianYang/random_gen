#ifndef STRATEGY_H
#define STRATEGY_H


class Strategy{};

#include "Generator.h"

class Strategy1D : public Strategy {
public:
  virtual void execute(PerlinNoise1D& perlin, double factor) = 0;
};

class AffineStrategy1D : public Strategy1D{
public:
  virtual void execute(PerlinNoise1D& perlin, double factor);
};

#endif
